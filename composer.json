{
    "name": "katsana/katsana-sdk-php",
    "description": "KATSANA SDK for PHP",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Sdk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "laravie/codex": "^3.1",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "php-http/curl-client": "^1.7",
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
